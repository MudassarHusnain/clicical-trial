generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Rate {
  id        Int      @id @default(autoincrement())
  ratAge    Int
  ratWeight Float
  ratTemp   Float
  createdAt DateTime @default(now())
  clerkId   String
}
